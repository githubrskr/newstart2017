<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2009 (http://www.altova.com) by Cap Gemini (Cap Gemini (UK) PLC) -->
<xsd:schema xmlns:subm="http://hmrc.gov.uk/etmp/digitalgateway/PODS" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://hmrc.gov.uk/etmp/digitalgateway/PODS" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:element name="PSA_SubscriptionReq" type="subm:PSASubscriptionRequestType">
		<xsd:annotation>
			<xsd:documentation>Root Element</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
 <xsd:complexType name="PSASubscriptionRequestType">
  <xsd:annotation>
   <xsd:documentation> </xsd:documentation>
  </xsd:annotation>
  <xsd:sequence>
   <xsd:element name="SubscriptionGeneric">
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="NonUKCustomer" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>Flag to indicate Non UK customer or not'true'=  Non UK Customer'false'= UK Customer</xsd:documentation>
       </xsd:annotation>
      </xsd:element>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="LegalEntityAndCustID">
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="LegalStatus">
       <xsd:annotation>
        <xsd:documentation>1 = Individual,
3 = Partnership,
7 = Limited Company
</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="2"/>
         <xsd:enumeration value="1"/>
         <xsd:enumeration value="3"/>
         <xsd:enumeration value="7"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="IDType" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>For Individual (01) - NINO,
For Limited Company (07)- UTR (CT UTR),
For Partnership (03)- UTR (SA UTR)</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="10"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="IDNumber" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>For Individual (01) - NINO,
For Limited Company (07)- UTR (CT UTR),
For Partnership (03)- UTR (SA UTR)</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="8"/>
         <xsd:maxLength value="15"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="NoIdentifier" minOccurs="1" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>'true'= select this flag for non-UK customers, because an identifier is not applicable for Non-UK customers,
'false '= select this flag for UK customers, because an identifier is mandatory for UK customer</xsd:documentation>
       </xsd:annotation>
      </xsd:element>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="OrganisationDetails" minOccurs="0">
    <xsd:annotation>
     <xsd:documentation>Mandatory if LegalStatus value 03 = Partnership OR  07 = Limited Company</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="OrganisationName" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation> </xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="160"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="CRNNumber" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation> </xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="7"/>
         <xsd:maxLength value="8"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="VATRegNumber" minOccurs="0">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="9"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="PAYEReference" minOccurs="0">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="16"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="IndividualDetails" minOccurs="0">
    <xsd:annotation>
     <xsd:documentation>Mandatory if LegalStatus value 01 = Individuals</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="Title" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>Possible values
0001: Mr
0002: Mrs
0003: Miss
0004: Ms
0005: Dr
0006: Sir
0009: Professor
0010: Lord </xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:length value="4"/>
         <xsd:enumeration value="0001"/>
         <xsd:enumeration value="0002"/>
         <xsd:enumeration value="0003"/>
         <xsd:enumeration value="0004"/>
         <xsd:enumeration value="0005"/>
         <xsd:enumeration value="0006"/>
         <xsd:enumeration value="0009"/>
         <xsd:enumeration value="0010"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="FirstName" minOccurs="1">
       <xsd:annotation>
        <xsd:documentation>First Name </xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="MiddleName" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>( Second Name also known as Middle Name )</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="LastName" minOccurs="1">
       <xsd:annotation>
        <xsd:documentation>Last Name</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="DateofBirth" type="xsd:date" minOccurs="1">
       <xsd:annotation>
        <xsd:documentation></xsd:documentation>
       </xsd:annotation>
      </xsd:element>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="ExistingPSAID">
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="ExistingPSAID" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>Flag to indicate  whether PSA has existing PSAID-
'true'= if ExistingPSAID is Yes,
'false'=  if ExistingPSAID is No
</xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="ExistingPSAIDReference" minOccurs="0">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:length value="8"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="CorrespondenceAddressDetails" minOccurs="1">
    <xsd:annotation>
     <xsd:documentation>CORRESPONDENCE CONTACT DETAILS SECTION</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
     <xsd:complexContent>
      <xsd:extension base="subm:AddressDetailsType"/>
     </xsd:complexContent>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="CorrespondenceContactDetails" minOccurs="1">
    <xsd:complexType>
     <xsd:complexContent>
      <xsd:extension base="subm:ContactDetailsType"/>
     </xsd:complexContent>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="PreviousAddressDetails">
    <xsd:annotation>
     <xsd:documentation>BUSINESS CONTACT DETAILS SECTION</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
     <xsd:complexContent>
      <xsd:extension base="subm:PreviousAddressType"/>
     </xsd:complexContent>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="NumOfDirOrPartnrsDetails" minOccurs="0">
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="DirectorsMorethan10" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>Applicable only for Limited company /organisation-
Select  ‘Yes’ when number of directors more than 10 partners,Select ‘No’  when number of directors less than or equal to 10</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="3"/>
         <xsd:enumeration value="Yes"/>
         <xsd:enumeration value="No"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="PartnersMorethan10" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>
Applicable only for partnership-
Select  ‘Yes’ when number of partners more than 10 partners,Select ‘No’  when number of partners less than or equal to 10</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="3"/>
         <xsd:enumeration value="Yes"/>
         <xsd:enumeration value="No"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="EntityAndDirOrPartnrDetails" maxOccurs="unbounded" minOccurs="0">
    <xsd:annotation>
     <xsd:documentation>Mandatory if LegalStatus value 03 = Partnership OR  07 = Limited Company ( Number of Directors Or Partners Nodes minimum 1 and Maximum 10 ) </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="SeqID">
       <xsd:annotation>
        <xsd:documentation>possible value format : 001</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:length value="3"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="EntityType">
       <xsd:annotation>
        <xsd:documentation>P-PartnerD-Director</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:length value="1"/>
         <xsd:enumeration value="P"/>
         <xsd:enumeration value="D"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Title" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>Possible values0001: Mr0002: Mrs0003: Miss0004: Ms0005: Dr0006: Sir0009: Professor0010: Lord</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:length value="4"/>
         <xsd:enumeration value="0001"/>
         <xsd:enumeration value="0002"/>
         <xsd:enumeration value="0003"/>
         <xsd:enumeration value="0004"/>
         <xsd:enumeration value="0005"/>
         <xsd:enumeration value="0006"/>
         <xsd:enumeration value="0009"/>
         <xsd:enumeration value="0010"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="FirstName" minOccurs="1">
       <xsd:annotation>
        <xsd:documentation></xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="MiddleName" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation></xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="LastName" minOccurs="1">
       <xsd:annotation>
        <xsd:documentation></xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="DateofBirth" type="xsd:date" minOccurs="1">
       <xsd:annotation>
        <xsd:documentation></xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="NINORef" minOccurs="0">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="9"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="IfnoNINOreason" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>Reason is mandatory if no NINO reference is provided</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="160"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="UTRRef" minOccurs="0">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:length value="10"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="IfnoUTRreason" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>Reason is mandatory if no UTR reference is provided</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="160"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="CorrespCommDeails" minOccurs="0">
       <xsd:complexType>
        <xsd:sequence>
         <xsd:element name="AddressDetails" type="subm:AddressDetailsType"/>
         <xsd:element name="ContactDetails" type="subm:ContactDetailsType"/>
        </xsd:sequence>
       </xsd:complexType>
      </xsd:element>
      <xsd:element name="PrevAddressDetails" minOccurs="0" type="subm:PreviousAddressType"/>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
   <xsd:element name="Declaration">
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="PSADeclarationBox1" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>PSADeclarationBox1 - For Additional Actual text / Description-  please refer related document in the EPID Appendix Section </xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="PSADeclarationBox2" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>PSADeclarationBox2 - For Additional Actual text / Description-  please refer related document in the EPID Appendix Section </xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="PSADeclarationBox3" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>PSADeclarationBox3 - For Additional Actual text / Description-  please refer related document in the EPID Appendix Section </xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="PSADeclarationBox4" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>PSADeclarationBox4 - For Additional Actual text / Description-  please refer related document in the EPID Appendix Section </xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="PSADeclarationBox5" type="xsd:boolean" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>If PSADeclarationBox5 is selected then PSADeclarationBox6 is an optional field. (For Additional Actual text / Description-  please refer related document in the EPID Appendix Section ) </xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="PSADeclarationBox6" type="xsd:boolean" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>If PSADeclarationBox6 is selected then provide pension advisor details and PSADeclarationBox5 is an optional field.(For Additional Actual text / Description-  please refer related document in the EPID Appendix Section)</xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="PensionAdviDeails" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>Pension Advisor details are mandatory if PSADeclarationBox6 is ‘true’ </xsd:documentation>
       </xsd:annotation>
       <xsd:complexType>
        <xsd:sequence>
         <xsd:element name="PensionAdviserName" minOccurs="1">
          <xsd:simpleType>
           <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="255"/>
           </xsd:restriction>
          </xsd:simpleType>
         </xsd:element>
         <xsd:element name="AddressDetails" type="subm:AddressDetailsType" minOccurs="1"/>
         <xsd:element name="ContactDetails" type="subm:ContactDetailsType" minOccurs="1"/>
        </xsd:sequence>
       </xsd:complexType>
      </xsd:element>
      <xsd:element name="PSADeclarationBox7" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>PSADeclarationBox7 - For Additional Actual text / Description-  please refer related document in the EPID Appendix Section </xsd:documentation>
       </xsd:annotation>
      </xsd:element>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
  </xsd:sequence>
 </xsd:complexType>
 <xsd:complexType name="AddressType">
  <xsd:sequence>
   <xsd:element name="AddressLine1">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="35"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="AddressLine2">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="35"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="AddressLine3" minOccurs="0">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="35"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="AddressLine4" minOccurs="0">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="35"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="PostCode" minOccurs="0">
    <xsd:annotation>
     <xsd:documentation>Post code field  Mandatory if address is UK</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="10"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="Country" type="subm:CountryType" minOccurs="0"/>
  </xsd:sequence>
 </xsd:complexType>
 <xsd:complexType name="AddressDetailsType">
  <xsd:sequence>
   <xsd:element name="NonUKAddress" type="xsd:boolean">
    <xsd:annotation>
     <xsd:documentation>‘true’=  Not UK address' fasle '=  UK address</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <xsd:element name="AddressLine1">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="35"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="AddressLine2">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="35"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="AddressLine3" minOccurs="0">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="35"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="AddressLine4" minOccurs="0">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="35"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="PostCode" minOccurs="0">
    <xsd:annotation>
     <xsd:documentation>Post code field  Mandatory if address is UK</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="10"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="CountryCode" type="subm:CountryType" minOccurs="1"/>
  </xsd:sequence>
 </xsd:complexType>
 <xsd:complexType name="PreviousAddressType">
  <xsd:sequence>
   <xsd:element name="PrevAddLast12mon" type="xsd:boolean">
    <xsd:annotation>
     <xsd:documentation>'true'=  Previous address selected'false'=  Previous address not selected</xsd:documentation>
    </xsd:annotation>
   </xsd:element>
   <xsd:element name="PrevAddressDetails" minOccurs="0">
    <xsd:annotation>
     <xsd:documentation>Mandatory if PrevAddLast12Mon = true</xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
     <xsd:sequence>
      <xsd:element name="NonUKAddress" type="xsd:boolean">
       <xsd:annotation>
        <xsd:documentation>true - If address is NOT in UK
false - Address is in the UK
</xsd:documentation>
       </xsd:annotation>
      </xsd:element>
      <xsd:element name="AddressLine1">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="AddressLine2">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="AddressLine3" minOccurs="0">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="AddressLine4" minOccurs="0">
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="35"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="PostCode" minOccurs="0">
       <xsd:annotation>
        <xsd:documentation>Post code field  Mandatory if address is UK</xsd:documentation>
       </xsd:annotation>
       <xsd:simpleType>
        <xsd:restriction base="xsd:string">
         <xsd:minLength value="1"/>
         <xsd:maxLength value="10"/>
        </xsd:restriction>
       </xsd:simpleType>
      </xsd:element>
      <xsd:element name="CountryCode" type="subm:CountryType" minOccurs="1"/>
     </xsd:sequence>
    </xsd:complexType>
   </xsd:element>
  </xsd:sequence>
 </xsd:complexType>
 <xsd:complexType name="ContactDetailsType">
  <xsd:sequence>
   <xsd:element name="Telephone">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:maxLength value="24"/>
      <xsd:minLength value="1"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="Mobile" minOccurs="0">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:maxLength value="24"/>
      <xsd:minLength value="1"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
   <xsd:element name="Fax" minOccurs="0">
    <xsd:simpleType>
     <xsd:restriction base="xsd:string">
      <xsd:maxLength value="24"/>
      <xsd:minLength value="1"/>
     </xsd:restriction>
    </xsd:simpleType>
   </xsd:element>
